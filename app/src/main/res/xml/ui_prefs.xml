<?xml version="1.0" encoding="utf-8"?><PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto">    <PreferenceCategory android:title="Network Speed " />    <PreferenceScreen android:title="Network traffic preferences">        <PreferenceCategory android:title="Network data">            <com.wubydax.romcontrol.v2.prefs.MyListPreference                android:defaultValue="3"                android:entries="@array/traffic_state_items"                android:entryValues="@array/traffic_state_values"                android:key="network_traffic_state"                android:title="Displayed traffic info"                app:dependentValue="0" />            <com.wubydax.romcontrol.v2.prefs.MySwitchPreference                android:defaultValue="false"                android:dependency="network_traffic_state"                android:key="network_traffic_unit"                android:summaryOff="Displayed in Bits (default)"                android:summaryOn="Displayed in Bytes"                android:title="Display data in Bytes" />        </PreferenceCategory>        <PreferenceCategory            android:dependency="network_traffic_state"            android:title="Arrows display preferences">            <com.wubydax.romcontrol.v2.prefs.MySwitchPreference                android:defaultValue="true"                android:dependency="network_traffic_state"                android:key="network_traffic_show_arrows"                android:summaryOff="Arrows hidden"                android:summaryOn="Arrows shown"                android:title="Show traffic arrows" />            <com.wubydax.romcontrol.v2.prefs.MyListPreference                android:defaultValue="0"                android:dependency="network_traffic_show_arrows"                android:entries="@array/arrow_position_entries"                android:entryValues="@array/arrow_position_values"                android:key="network_traffic_arrows_position"                android:title="Arrows position" />        </PreferenceCategory>        <PreferenceCategory            android:dependency="network_traffic_state"            android:title="Display options">            <com.wubydax.romcontrol.v2.prefs.MySwitchPreference                android:defaultValue="false"                android:key="network_traffic_autohide"                android:summaryOff="Displayed always"                android:summaryOn="Autohide enabled"                android:title="Autohide on low traffic" />            <com.wubydax.romcontrol.v2.prefs.MySeekBarPreference                android:defaultValue="10"                android:dependency="network_traffic_autohide"                android:key="network_traffic_threshold"                android:title="Autohide threshold"                app:maxValue="100"                app:unitsValue="Kb" />            <com.wubydax.romcontrol.v2.prefs.MyListPreference                android:defaultValue="0"                android:entries="@array/traffic_style_entries"                android:entryValues="@array/traffic_style_entry_values"                android:key="network_traffic_text_style"                android:title="Font style" />            <com.wubydax.romcontrol.v2.prefs.MySeekBarPreference                android:defaultValue="10"                android:key="network_traffic_scale"                android:title="Text size scale factor"                app:maxValue="10" />        </PreferenceCategory>        <PreferenceCategory            android:title="Network traffic colors"            android:dependency="network_traffic_state">            <com.wubydax.romcontrol.v2.prefs.MySwitchPreference                android:title="Separate colors for elements"                android:key="network_traffic_global_color"                android:defaultValue="true"                android:summaryOn="All elements can be colored separately"                android:summaryOff="Global color will apply to all elements"/>            <com.wubydax.romcontrol.v2.prefs.ColorPickerPreference                android:title="Global color"                android:key="network_traffic_color"                android:defaultValue="#ffffff"/>            <com.wubydax.romcontrol.v2.prefs.ColorPickerPreference                android:title="Upload text color"                android:defaultValue="#ffffff"                android:key="network_traffic_upload_color"                android:dependency="network_traffic_global_color"/>            <com.wubydax.romcontrol.v2.prefs.ColorPickerPreference                android:title="Download text color"                android:defaultValue="#ffffff"                android:key="network_traffic_download_color"                android:dependency="network_traffic_global_color"/>            <com.wubydax.romcontrol.v2.prefs.ColorPickerPreference                android:title="Traffic arrows color"                android:key="network_traffic_arrow_color"                android:defaultValue="#ffffff"                android:dependency="network_traffic_global_color"/>        </PreferenceCategory>    </PreferenceScreen>    <PreferenceCategory android:title="Matrix Animations Features " />    <PreferenceScreen android:title="Matrix Elements" android:summary="Matrix Options">        <com.wubydax.romcontrol.v2.prefs.MySwitchPreference android:title="Matrix Recents Anim " android:key="matrix_anim" android:summaryOn="Enabled " android:summaryOff="Disabled"  />        <com.wubydax.romcontrol.v2.prefs.ColorPickerPreference android:title="Matrix Text Colorized " android:dependency="matrix_anim" android:summary="Choose Matrix Text color" android:key="recents_matrix_anim_text_color" android:defaultValue="\#ff286fdd" alphaSlider="true" />        <com.wubydax.romcontrol.v2.prefs.ColorPickerPreference android:title="Matrix BG Color" android:dependency="matrix_anim" android:summary="Choose Matrix BG color" android:key="recents_matrix_anim_bg_color" android:defaultValue="\#ff000000" alphaSlider="true" />        <com.wubydax.romcontrol.v2.prefs.MySeekBarPreference android:max="255" android:title="Matrix Text Transparency" android:dependency="matrix_anim" android:key="recents_matrix_anim_text_alpha" android:defaultValue="255" min="5" unitsRight="" />        <com.wubydax.romcontrol.v2.prefs.MySeekBarPreference android:max="50" android:title="Matrix Text Font Size" android:dependency="matrix_anim" android:key="recents_matrix_anim_font_size" android:defaultValue="40" min="24" unitsRight="" />        <com.wubydax.romcontrol.v2.prefs.MySeekBarPreference android:max="50" android:title="Matrix Frequency" android:dependency="matrix_anim" android:key="recents_matrix_anim_frequency" android:defaultValue="5" min="1" unitsRight="" />    </PreferenceScreen>    <PreferenceCategory android:title="Mokee Listview Animation" android:summary="Set Scrolling Animation">    <com.wubydax.romcontrol.v2.prefs.MyListPreference android:entries="@array/listview_animation_entries" android:title="Listview animation" android:key="listview_animation" android:dependency="listview_animation" android:defaultValue="0" android:entryValues="@array/listview_animation_values" />    <com.wubydax.romcontrol.v2.prefs.MyListPreference android:entries="@array/listview_interpolator_entries" android:title="Listview interpolator" android:key="listview_interpolator" android:dependency="listview_interpolator" android:defaultValue="0" android:entryValues="@array/listview_interpolator_values" />    </PreferenceCategory>    <PreferenceCategory android:title="Custom ToastAnimations">        <com.wubydax.romcontrol.v2.prefs.MyListPreference android:entries="@array/toast_animation_entries" android:title="Toast Animations UI Framework " android:key="animation_toast" android:summary="%s" android:defaultValue="0" android:entryValues="@array/toast_animation_values" />        <com.wubydax.romcontrol.v2.prefs.RunScriptPreference android:title="Toast Test" android:summary="Shows selected Animation" app:scriptFileName="test.sh" />    </PreferenceCategory>    <PreferenceCategory android:title="BattreyBar Settings" />    <PreferenceScreen android:title="Battery Bar" android:summary="Display Line on top for battery" />    <com.wubydax.romcontrol.v2.prefs.MyListPreference android:entries="@array/battery_bar_entries" android:title="Battery Bar" android:key="battery_bar" android:summary="%s" android:defaultValue="2" android:entryValues="@array/battery_bar_values" />    <com.wubydax.romcontrol.v2.prefs.MySwitchPreference android:title="Battery bar animation" android:key="battery_bar_animate" android:defaultValue="true" android:summaryOn="Charging animation enabled" android:summaryOff="Animation disabled" />    <com.wubydax.romcontrol.v2.prefs.MyListPreference android:entries="@array/battery_bar_style_entries" android:title="Battery bar style" android:key="battery_bar_style" android:summary="%s" android:defaultValue="1" android:entryValues="@array/battery_bar_style_values" />    <com.wubydax.romcontrol.v2.prefs.MyListPreference android:entries="@array/battery_bar_thickness_entries" android:title="Thickness of battery bar" android:key="battery_bar_thickness" android:summary="%s" android:defaultValue="2" android:entryValues="@array/battery_bar_thickness_values" />    <PreferenceCategory android:title="status bar icon colors" />    <com.wubydax.romcontrol.v2.prefs.ColorPickerPreference android:title="StatusBar icon colors" android:key="sb_icon_color"  android:defaultValue="\#ffffffff" alphaSlider="true" hexValue="true" /><PreferenceCategory android:title="battery look" />    <com.wubydax.romcontrol.v2.prefs.MySwitchPreference android:title="Stock Battery" android:key="hide_battery_stock" android:defaultValue="true" android:summaryOn="Show" android:summaryOff="Hide" />    <com.wubydax.romcontrol.v2.prefs.MySwitchPreference android:title="Battery Percentage" android:key="display_battery_percentage" android:defaultValue="true" android:summaryOn="Show" android:summaryOff="Hide" />    <com.wubydax.romcontrol.v2.prefs.MySwitchPreference android:title="3Minit Battery" android:key="minit_battery_visible" android:defaultValue="false" android:summaryOn="Show" android:summaryOff="Hide" />    <com.wubydax.romcontrol.v2.prefs.ColorPickerPreference        android:title="Percentage Color"        android:key="battery_text_color"        android:defaultValue="#ffffffff"        android:summary="Choose battery percent color" />    <PreferenceCategory android:title="Ram Bar" />    <SwitchPreference android:title="RAM Bar on Recent App" android:key="aokpram_recent" android:defaultValue="true" android:summaryOn="Shown" android:summaryOff="Hidden" />    <com.wubydax.romcontrol.v2.prefs.ColorPickerPreference android:title="Used Ram Text Color" android:key="ram_color1" android:defaultValue="\#ffffffff" app:packageNameToKill="com.android.systemui" app:isSilent="false" alphaSlider="true" hexValue="true" />    <com.wubydax.romcontrol.v2.prefs.ColorPickerPreference android:title="Free Ram Text Color" android:key="ram_color2" android:defaultValue="\#ffffffff" app:packageNameToKill="com.android.systemui" app:isSilent="false" alphaSlider="true" hexValue="true" /></PreferenceScreen>